{
  "manifest_version": 2,
  "name": "Close Tabs to the Left",
  "short_name": "Close Left",
  "description": "Adds \"Close Tabs to the Left\" button on tab right-click menus.",
  "version": "0.0.1",
  "homepage_url": "https://github.com/ParkerM/close-tabs-to-left",
  "developer": {
    "name": "Parker Mauney",
    "url": "https://github.com/ParkerM/close-tabs-to-left"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "closetabstotheleft@parkerm.github.io",
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
      "scripts": ["dist/background.js"]
  },
  "permissions": [
    "menus",
    "tabs"
  ]
}
